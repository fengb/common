syntax on
colors ir_black
filetype indent plugin on

set number

set tabstop=8
set shiftwidth=2
set softtabstop=2
set expandtab

set autoindent

set backupdir=~/.vimbak
set directory=~/.vimtmp

set list
set listchars=tab:>-,trail:.

set colorcolumn=81
set hlsearch

let g:netrw_dirhistmax=0

execute pathogen#infect()
