<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<array>
	<dict>
		<key>keyword</key>
		<string>morningstar</string>
		<key>spaces</key>
		<false/>
		<key>text</key>
		<string>Search Morningstar for '{query}'</string>
		<key>url</key>
		<string>http://quote.morningstar.com/Quote/Quote.aspx?ticker={query}</string>
		<key>utf8</key>
		<false/>
	</dict>
	<dict>
		<key>keyword</key>
		<string>marketwatch</string>
		<key>spaces</key>
		<false/>
		<key>text</key>
		<string>Search MarketWatch for '{query}'</string>
		<key>url</key>
		<string>http://www.marketwatch.com/tools/quotes/lookup.asp?lookup={query}</string>
		<key>utf8</key>
		<false/>
	</dict>
	<dict>
		<key>keyword</key>
		<string>finance</string>
		<key>spaces</key>
		<false/>
		<key>text</key>
		<string>Search Google Finance with '{query}'</string>
		<key>url</key>
		<string>http://www.google.com/finance?q={query}</string>
		<key>utf8</key>
		<false/>
	</dict>
	<dict>
		<key>keyword</key>
		<string>sec</string>
		<key>spaces</key>
		<false/>
		<key>text</key>
		<string>Search SEC Edgar with '{query}'</string>
		<key>url</key>
		<string>http://sec.gov/cgi-bin/browse-edgar?owner=exclude&amp;Find=Find+Companies&amp;action=getcompany&amp;CIK={query}</string>
		<key>utf8</key>
		<false/>
	</dict>
	<dict>
		<key>keyword</key>
		<string>seekingalpha</string>
		<key>spaces</key>
		<false/>
		<key>text</key>
		<string>Search Seeking Alpha for</string>
		<key>url</key>
		<string>http://seekingalpha.com/search/{query}</string>
		<key>utf8</key>
		<false/>
	</dict>
</array>
</plist>
